{
  "typescript.tsdk": "node_modules/typescript/lib",
  "files.watcherExclude": {
    "**/.git/objects/**": true,
    "**/.git/subtree-cache/**": true,
    "**/node_modules/*/**": true,
    "**/.hg/store/**": true,
    "**/.next/**": true,
    "**/.yarn/cache/**": true,
    "**/.cache/**": true,
    "**/.turbo/**": true,
    "**/package-lock.json": true,
    "**/yarn.lock": true,
    "**/dist/*/**": true,
    "**/build/*/**": true,
    "**/vendor/*/**": true,
    "**/dist_electron/**": true,
    "**/.eslintcache": true,
    "**/__snapshots__/**": true
  },
  "editor.defaultFormatter": "esbenp.prettier-vscode",
  "todo-tree.highlights.useColourScheme": true,
  "todo-tree.general.tags": [
    "BUG",
    "Bug:",
    "HACK",
    "Hack:",
    "WARN",
    "Warn:",
    "ERR",
    "ERROR",
    "FIXME",
    "FIXIT",
    "FIX",
    "Fix:",
    "TODO",
    "XXX",
    "DELETE",
    "Delete:",
    "[ ]",
    "[x]",
    "NOTE",
    "IDEA",
    "THOUGHT",
    "REVIEW",
    "Review:",
    "Note:",
    "Idea:",
    "Thought:",
    "Info:",
    "INFO",
    "BLOCKED",
    "Blocked:",
    "BLOCKER",
    "Blocker:",
    "FUTURE",
    "PENDING",
    "REFACTOR",
    "Refactor:"
  ],
  "todo-tree.general.tagGroups": {
    "WARN": ["WARN", "ERR", "ERROR", "Warn:"],
    "NOTE": [
      "NOTE",
      "IDEA",
      "THOUGHT",
      "Note:",
      "Idea:",
      "Thought:",
      "Info:",
      "INFO"
    ],
    "FIXME": ["FIXME", "FIXIT", "FIX", "Fix:"],
    "FUTURE": ["FUTURE", "PENDING"],
    "REFACTOR": ["REFACTOR", "Refactor:", "Review:", "REVIEW"],
    "BUG": ["BUG", "Bug:"],
    "HACK": ["HACK", "Hack:"],
    "BLOCKED": ["BLOCKED", "Blocked:", "Blocker:", "BLOCKER"],
    "XXX": ["XXX", "Delete:", "DELETE"]
  },
  "todo-tree.regex.regex": "(//|#|<!--|;|/\\*|^|^\\s*(-|\\d+.))\\s*($TAGS)",
  "todo-tree.highlights.defaultHighlight": {},
  "todo-tree.highlights.customHighlight": {
    "BUG": {
      "foreground": "#ffffff",
      "background": "#DC143C",
      "iconColour": "#DC143C",
      "gutterIcon": true,
      "icon": "bug"
    },
    "TODO": {
      "foreground": "#ffffff",
      "background": "#3CB371",
      "iconColour": "#3CB371",
      "gutterIcon": true,
      "icon": "check"
    },
    "[ ]": {
      "foreground": "#ffffff",
      "background": "#c71c47",
      "iconColour": "#c71c47",
      "gutterIcon": true,
      "icon": "check-circle"
    },
    "[x]": {
      "foreground": "#ffffff",
      "background": "#3CB371",
      "iconColour": "#3CB371",
      "gutterIcon": true,
      "icon": "check-circle-fill"
    },
    "FIXME": {
      "foreground": "#ffffff",
      "background": "#1E90FF",
      "iconColour": "#1E90FF",
      "gutterIcon": true,
      "icon": "tools"
    },
    "WARN": {
      "foreground": "#000000",
      "background": "#FFD700",
      "iconColour": "#FFD700",
      "gutterIcon": true,
      "icon": "alert"
    },
    "HACK": {
      "foreground": "#000000",
      "background": "#FF8C00",
      "iconColour": "#FF8C00",
      "gutterIcon": true,
      "icon": "beaker"
    },
    "XXX": {
      "foreground": "#000000",
      "background": "#B22222",
      "iconColour": "#B22222",
      "gutterIcon": true,
      "icon": "x",
      "fontStyle": "bold"
    },
    "FUTURE": {
      "foreground": "#ffffff",
      "background": "indigo",
      "iconColour": "darkviolet",
      "gutterIcon": true,
      "icon": "clock"
    },
    "NOTE": {
      "background": "darkcyan",
      "iconColour": "darkcyan",
      "type": "comment",
      "gutterIcon": true,
      "icon": "info",
      "fontStyle": "italic"
    },
    "REFACTOR": {
      "foreground": "#ffffff",
      "background": "MediumOrchid",
      "iconColour": "MediumOrchid",
      "gutterIcon": true,
      "icon": "code"
    },
    "BLOCKED": {
      "foreground": "#ffffff",
      "background": "darkred",
      "iconColour": "red",
      "gutterIcon": true,
      "icon": "circle-slash"
    }
  },
  "todo-tree.general.statusBar": "current file",
  "todo-tree.filtering.excludeGlobs": [
    "**/node_modules",
    "**/{temp,tmp,dist,build}",
    "**/.*",
    "**/*.config.{js,json}",
    "**/*.{min,backup.temp}.*"
  ],
  "todo-tree.filtering.useBuiltInExcludes": "file and search excludes",
  "files.exclude": {
    "yarn.lock": false,
    ".husky": true,
    ".idea": true
  },
  "cSpell.words": [
    "autodocs",
    "docgen",
    "gquittet",
    "healthz",
    "msgpack",
    "msgpackr",
    "nextjs",
    "rabbitmq",
    "typebox"
  ],
  "eslint.packageManager": "yarn",
  "editor.codeActionsOnSave": {
    "source.fixAll.eslint": true
  },
  "eslint.codeActionsOnSave.mode": "problems",
  "eslint.format.enable": true,
  "eslint.lintTask.enable": true,
  "eslint.runtime": "node",
  "eslint.trace.server": "verbose",
  "eslint.useESLintClass": true,
  "eslint.validate": [
    "javascript",
    "javascriptreact",
    "typescript",
    "typescriptreact"
  ],
  "eslint.workingDirectories": [
    {
      "mode": "auto"
    }
  ]
}
